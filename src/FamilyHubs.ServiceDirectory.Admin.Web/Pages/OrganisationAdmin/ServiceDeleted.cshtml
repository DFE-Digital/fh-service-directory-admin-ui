@page
@using Microsoft.AspNetCore.Mvc.TagHelpers
@model FamilyHubs.ServiceDirectory.Admin.Web.Pages.OrganisationAdmin.ServiceDeletedModel

<main class="govuk-main-wrapper govuk-main-wrapper--auto-spacing" id="main-content" role="main">
    <form method="post">
        <div class="govuk-grid-row">
            <div class="govuk-grid-column-two-thirds">
                <h1 class="govuk-heading-l">You have deleted the service</h1>
                <p>It is no longer showing in the directory.</p>
                <p>You can add a service from your homepage.</p>
                <button type="submit" class="govuk-button">
                    Go to home page
                </button>
            </div>
        </div>
    </form>
</main>