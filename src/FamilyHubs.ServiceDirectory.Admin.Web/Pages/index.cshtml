@page
@model IndexModel
@{
    ViewData["Title"] = "Manage family support services and accounts";
}

<main class="govuk-main-wrapper govuk-main-wrapper--auto-spacing" id="main-content" role="main">
    <div class="govuk-grid-row">
        <div class="govuk-grid-column-two-thirds">

            <h1 class="govuk-heading-l">Manage family support services and accounts</h1>

            <p>This is to manage family support services.</p>
            <p>Use this to:</p>
            <ul class="govuk-list govuk-list--bullet">
                <li>add local authorities</li>
                <li>add VCS organisations to the directory</li>
                <li>view organisations you have added to the directory </li>
            </ul>

            <a asp-page="/Welcome" class="govuk-button govuk-button--start">
                Start now
                <svg class="govuk-button__start-icon" xmlns="http://www.w3.org/2000/svg" width="17.5" height="19" viewBox="0 0 33 40" aria-hidden="true" focusable="false">
                    <path fill="currentColor" d="M0 0h13l20 20-20 20H0l20-20z"></path>
                </svg>
            </a>
        </div>

    </div>
</main>