@page
@model FamilyHubs.ServiceDirectory.Admin.Web.Areas.Locations.Pages.ManageLocationsModel
@{
    ViewData["Title"] = Model.Title;
}

@section Back {
    <a href="/Welcome" class="govuk-back-link">Back</a>
}

<div class="govuk-grid-row">
    <div class="govuk-grid-column-two-thirds">
        <h1 class="govuk-heading-l">@Model.Title</h1>
        <p>@Model.SubTitle</p>
    </div>
</div>
@if(Model.ResultCount > 0)
{
    <partial name="_Dashboard" model='Model' />
}
else
{
    <h2 class="govuk-heading-m">No results returned</h2>
}

