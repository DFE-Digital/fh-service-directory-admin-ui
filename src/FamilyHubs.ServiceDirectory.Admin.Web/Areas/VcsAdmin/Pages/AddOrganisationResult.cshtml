@page
@model FamilyHubs.ServiceDirectory.Admin.Web.Areas.VcsAdmin.Pages.AddOrganisationResultModel
@{
    ViewData["Title"] = "Voluntary community organisation added";
}

<div class="govuk-grid-row">
    <div class="govuk-grid-column-two-thirds">
        <div class="govuk-panel govuk-panel--confirmation">
            <h1 class="govuk-panel__title">
                Voluntary community organisation added
            </h1>
        </div>

        <h2 class="govuk-heading-m govuk-!-margin-top-6">What happens next</h2>

        <p>
            @Model.DisplayMessage
        </p>
        <p>You can do this from your homepage.</p>
        <form method="post">
            <a asp-page="welcome" id="buttonGoToHome" class="govuk-button">Go to homepage</a>
        </form>

    </div>
</div>
